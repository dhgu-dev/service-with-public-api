  "private": true,
  "homepage": "https://dhgu-dev.github.io/service-with-public-api/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.32",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.7",
    "react-icons": "^4.8.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "recoil": "^0.7.7",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.0.0-rc.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:storybook/recommended"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.+(css|module.css|styles.ts)": [
      "stylelint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.14",
    "@storybook/addon-interactions": "^7.0.14",
    "@storybook/addon-links": "^7.0.14",
    "@storybook/blocks": "^7.0.14",
    "@storybook/preset-create-react-app": "^7.0.14",
    "@storybook/react": "^7.0.14",
    "@storybook/react-webpack5": "^7.0.14",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react-slick": "^0.23.10",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.8",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.0.14",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0",
    "webpack": "^5.83.1"
  }
